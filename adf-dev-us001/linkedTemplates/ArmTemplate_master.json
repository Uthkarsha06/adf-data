{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-us001"
		},
		"ls_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2'"
		},
		"ls_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sa'"
		},
		"ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen002.dfs.core.windows.net/"
		},
		"ls_http_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-ind-dev.vault.azure.net/"
		},
		"ls_sas_gen2_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'ls_sas_gen2'"
		},
		"ls_sas_gen2_properties_typeProperties_sasToken_secretName": {
			"type": "string",
			"defaultValue": "SAS-adlsgen002"
		},
		"tr_storage_event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/515c18d9-5b6f-4dab-aea8-055d5ebb868c/resourceGroups/rg-adls2/providers/Microsoft.Storage/storageAccounts/adlsgen002"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-us001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adlsgen2_accountKey": {
						"value": "[parameters('ls_adlsgen2_accountKey')]"
					},
					"ls_sa_connectionString": {
						"value": "[parameters('ls_sa_connectionString')]"
					},
					"ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_http_git_properties_typeProperties_url": {
						"value": "[parameters('ls_http_git_properties_typeProperties_url')]"
					},
					"ls_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_sas_gen2_sasUri": {
						"value": "[parameters('ls_sas_gen2_sasUri')]"
					},
					"ls_sas_gen2_properties_typeProperties_sasToken_secretName": {
						"value": "[parameters('ls_sas_gen2_properties_typeProperties_sasToken_secretName')]"
					},
					"tr_storage_event_properties_typeProperties_scope": {
						"value": "[parameters('tr_storage_event_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}